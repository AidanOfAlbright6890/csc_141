def make_model(manufacturer, *models):
    """Summarize the car we are about to buy."""
    print(f"\ndescribing the {manufacturer}-company car we will buy:")
    for model in models:
        print(f"- {model}")





from models import make_model as mm
mm('Audi', 'lamborghini')
mm('ferari', '44 8 Spider')
    














